<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NeumorphismConstraintView">
        <attr name="neumorphismClickable" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="JuYeonSwitch">
        <attr name="onImage" format="reference"/>
        <attr name="offImage" format="reference"/>
        <attr name="switchColor" format="color"/>
        <attr name="switchOn" format="boolean"/>
        <attr name="backgroundColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="JuYeonDropDown">
        <attr name="backgroundColour" format="color" />
        <attr name="textColour" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="text" format="string" />
        <attr name="emoji" format="string" />
    </declare-styleable>

    <declare-styleable name="JuYeonNumberPicker">
        <attr name="itemHeight" format="dimension" />
    </declare-styleable>
</resources>